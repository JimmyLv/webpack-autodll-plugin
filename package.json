{
  "name": "@parabol/webpack-autodll-plugin",
  "version": "0.0.0-development",
  "description": "AutoDLL Webpack Plugin for handling DLL caching outside of webpack.",
  "main": "src/index.js",
  "files": [
    "package.json",
    "README.md",
    "dist/",
    "docs/",
    "images/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "sucrase ./src -d ./dist --transforms imports",
    "pretest": "rm -rf dev/dll",
    "test": "jest",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyLv/webpack-autodll-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "dll",
    "cache",
    "optimize",
    "parabol"
  ],
  "author": "JimmyLv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JimmyLv/webpack-autodll-plugin/issues"
  },
  "homepage": "https://github.com/JimmyLv/webpack-autodll-plugin#readme",
  "dependencies": {
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@sucrase/jest-plugin": "^2.2.0",
    "bluebird": "^3.7.2",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "memory-fs": "^0.5.0",
    "prettier": "^2.4.1",
    "semantic-release": "19.0.2",
    "semantic-release-gitmoji": "^1.3.4",
    "simple-git-hooks": "^2.7.0",
    "sucrase": "^3.20.3",
    "webpack-defaults": "^4.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.js": "prettier --write"
  }
}
